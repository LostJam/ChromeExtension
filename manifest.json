{
  "name": "KSDT Live",
  "version": "1.1",
  "description": "Chrome extension to listen to the live KSDT stream. AKA listen to some groovy tunes, man.",
  "manifest_version": 2,

  "browser_action":{
    "default_icon": "ksdtBus.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
      "activeTab", "https://ksdt.ucsd.edu/stream.mp3"
    ]
}
 